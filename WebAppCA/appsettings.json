{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Grpc": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "WebAppCA.Services": "Debug",
      "WebAppCA.Extensions": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=WebAppCA;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "GrpcSettings": {
    "Address": "localhost",
    "Port": 4000,
    "CaCertPath": "../../../../Cert/gateway/ca.crt",
    "UseSSL": false
  },
  "DeviceGateway": {
    "CaCertPath": "../../../../Cert/gateway/ca.crt",
    "Address": "localhost",
    "Port": 4000
  }
}
