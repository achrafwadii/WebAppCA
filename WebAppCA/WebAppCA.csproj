<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>
<ItemGroup>
  <Content Include="BioStarSDK\**\*.dll" CopyToOutputDirectory="PreserveNewest" />
</ItemGroup>
<ItemGroup>
  <PackageReference Include="MartinCostello.Logging.XUnit" Version="0.5.1" />
  <PackageReference Include="Microsoft.EntityFrameworkCore" Version="9.0.4" />
  <PackageReference Include="Microsoft.Extensions.Logging" Version="9.0.4" />
  <PackageReference Include="System.IO.Ports" Version="9.0.4" />
  <PackageReference Include="System.Net.Http" Version="4.3.4" />
</ItemGroup>
<ItemGroup>
	<Content Update="BioStarSDK\lib\BS_SDK_V2_x64.dll">
		<CopyToOutputDirectory>Always</CopyToOutputDirectory>
	</Content>
  <Content Update="BioStarSDK\lib\BS_SDK_V2.dll">
    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
  </Content>
  <Content Update="BioStarSDK\lib\libcrypto-1_1-x64.dll">
    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
  </Content>
  <Content Update="BioStarSDK\lib\libssl-1_1-x64.dll">
    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
  </Content>
</ItemGroup>
</Project>
