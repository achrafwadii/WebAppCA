{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Grpc": "Debug",
      "System.Net.Http.HttpClient": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=WebAppCA;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "GrpcSettings": {
    "Address": "localhost",
    "Port": 4000,
    "CaCertPath": "Certs/ca.crt",
    "ServerCertPath": "Certs/server.crt",
    "ServerKeyPath": "Certs/server_key.pem",
    "UseSSL": true,
    "IgnoreCertErrors": true,
    "RetryAttempts": 3,
    "RetryDelaySeconds": 5
  },
  "DeviceGateway": {
    "CaCertPath": "Certs/ca.crt",
    "Address": "localhost",
    "Port": 4000
  },
  "PortSettings": {
    "HttpPort": "80",
    "HttpsPort": "443",
    "WebSocketPort": "9002",
    "ApiPort": "9010",
    "DatabasePort": "1433",
    "TaHttpPort": "3000",
    "TaHttpsPort": "3002",
    "TaCloudPort": "52001",
    "FastCgiPort": "9000",
    "UnifiedGatewayHttpPort": "5000",
    "UnifiedGatewayHttpsPort": "5002"
  },
  "SerialPort": {
    "Name": "",
    "BaudRate": "9600",
    "DataBits": "8",
    "Parity": "None",
    "StopBits": "One"
  }

}